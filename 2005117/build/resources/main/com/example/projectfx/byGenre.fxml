<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #2a363b;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.projectfx.ByGenreController">
   <children>
      <Label layoutX="138.0" layoutY="166.0" prefHeight="41.0" prefWidth="115.0" style="-fx-background-color: #e2d4d1;" text="    Enter a Genre" textFill="#171717">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <TextField fx:id="genre" layoutX="279.0" layoutY="169.0" prefHeight="35.0" prefWidth="149.0" promptText="Genre" style="-fx-background-color: white;" />
      <Button fx:id="back" layoutX="40.0" layoutY="333.0" mnemonicParsing="false" onAction="#backClicked" text="Go back">
         <effect>
            <Lighting>
               <bumpInput>
                  <Shadow />
               </bumpInput>
               <light>
                  <Light.Distant />
               </light>
            </Lighting>
         </effect>
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <Button fx:id="submit" layoutX="501.0" layoutY="333.0" mnemonicParsing="false" onAction="#submitClicked" prefHeight="25.0" prefWidth="62.0" text="submit">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
         <effect>
            <Lighting>
               <bumpInput>
                  <Shadow />
               </bumpInput>
               <light>
                  <Light.Distant />
               </light>
            </Lighting>
         </effect></Button>
   </children>
</AnchorPane>
