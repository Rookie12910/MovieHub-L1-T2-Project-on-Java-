         <font>
            <Font name="System Bold" size="18.0" />
         </font>
         <effect>
            <Lighting>
               <bumpInput>
                  <Shadow />
               </bumpInput>
               <light>
                  <Light.Distant />
               </light>
            </Lighting>
         </effect></Button>
      <ImageView fitHeight="405.0" fitWidth="413.0" layoutX="190.0" pickOnBounds="true" preserveRatio="true">
