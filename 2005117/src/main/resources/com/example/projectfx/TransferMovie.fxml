<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #2a363b;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.projectfx.TransferMovieController">
   <children>
      <Label layoutX="65.0" layoutY="122.0" prefHeight="38.0" prefWidth="186.0" style="-fx-background-color: #e2d4d1;" text="           Enter Movie Title">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <TextField fx:id="movieTitle" layoutX="263.0" layoutY="123.0" prefHeight="38.0" prefWidth="211.0" promptText="Movie Title" />
      <Label layoutX="66.0" layoutY="188.0" prefHeight="38.0" prefWidth="186.0" style="-fx-background-color: #e2d4d1;" text="Transfer To (Production Company)">
         <font>
            <Font name="System Bold" size="11.0" />
         </font></Label>
      <TextField fx:id="productionCompany" layoutX="264.0" layoutY="189.0" prefHeight="38.0" prefWidth="211.0" promptText="Production Company" />
      <Button fx:id="submit" layoutX="488.0" layoutY="327.0" mnemonicParsing="false" onAction="#submitClicked" text="Submit">
         <effect>
            <Lighting>
               <bumpInput>
                  <Shadow />
               </bumpInput>
               <light>
                  <Light.Distant />
               </light>
            </Lighting>
         </effect>
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <Button fx:id="back" layoutX="32.0" layoutY="327.0" mnemonicParsing="false" onAction="#backClicked" text="Go Back">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
         <effect>
            <Lighting>
               <bumpInput>
                  <Shadow />
               </bumpInput>
               <light>
                  <Light.Distant />
               </light>
            </Lighting>
         </effect></Button>
   </children>
</AnchorPane>
