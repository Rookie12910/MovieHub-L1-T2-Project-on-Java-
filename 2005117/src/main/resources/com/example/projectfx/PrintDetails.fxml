<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #2a363b;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.projectfx.PrintDetailsController">
   <children>
      <TableView fx:id="movieDetails" layoutY="2.0" prefHeight="327.0" prefWidth="600.0">
         <columns>
            <TableColumn fx:id="title" prefWidth="160" text="Title" />
            <TableColumn fx:id="year" prefWidth="100" text=" Release Year" />
            <TableColumn fx:id="company" prefWidth="265" text="Production Company" />
            <TableColumn fx:id="details" prefWidth="75.0" text="Details" />
         </columns></TableView>
      <Button layoutX="24.0" layoutY="348.0" mnemonicParsing="false" onAction="#goBackClicked" text="Go Back">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
         <effect>
            <Lighting>
               <bumpInput>
                  <Shadow />
               </bumpInput>
               <light>
                  <Light.Distant />
               </light>
            </Lighting>
         </effect></Button>
      <Button layoutX="508.0" layoutY="347.0" mnemonicParsing="false" onAction="#refreshClicked" text="Refresh">
         <font>
            <Font name="System Bold" size="12.288000106811523" />
         </font>
         <effect>
            <Lighting>
               <bumpInput>
                  <Shadow />
               </bumpInput>
               <light>
                  <Light.Distant />
               </light>
            </Lighting>
         </effect></Button>
   </children>
</AnchorPane>
