<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #2a363b;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.projectfx.ByRunningTimeController">
   <children>
      <Label layoutX="146.0" layoutY="149.0" prefHeight="56.0" prefWidth="133.0" style="-fx-background-color: #e2d4d1;" text="  Enter Time Range">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <TextField fx:id="initialTime" layoutX="300.0" layoutY="149.0" promptText="Initial Time" />
      <TextField fx:id="finalTime" layoutX="300.0" layoutY="188.0" promptText="Final Time" />
      <Button fx:id="submit" layoutX="498.0" layoutY="330.0" mnemonicParsing="false" onAction="#submitClicked" text="Submit">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
         <effect>
            <Lighting>
               <bumpInput>
                  <Shadow />
               </bumpInput>
               <light>
                  <Light.Distant />
               </light>
            </Lighting>
         </effect></Button>
      <Button fx:id="back" layoutX="42.0" layoutY="330.0" mnemonicParsing="false" onAction="#backClicked" text="Go Back">
         <effect>
            <Lighting>
               <bumpInput>
                  <Shadow />
               </bumpInput>
               <light>
                  <Light.Distant />
               </light>
            </Lighting>
         </effect></Button>
   </children>
</AnchorPane>
